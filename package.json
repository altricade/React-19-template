{
  "name": "react-19-ssr-template",
  "version": "1.0.0",
  "description": "React 19 template with SSR, Redux Toolkit, Tailwind, and more",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server/index.js",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\" ",
    "dev:client": "webpack serve --config webpack.client.dev.js",
    "dev:server": "webpack --config webpack.server.dev.js --watch & nodemon dist/server/index.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --config webpack.client.prod.js",
    "build:server": "webpack --config webpack.server.prod.js",
    "build:dev": "npm run build:client:dev && npm run build:server:dev",
    "build:client:dev": "webpack --config webpack.client.dev.js",
    "build:server:dev": "webpack --config webpack.server.dev.js",
    "test": "jest",
    "docker:build:dev": "docker build -t react-19-ssr-template:dev --build-arg NODE_ENV=development .",
    "docker:build:prod": "docker build -t react-19-ssr-template:prod --build-arg NODE_ENV=production .",
    "docker:run:dev": "docker run -p 3000:3000 react-19-ssr-template:dev",
    "docker:run:prod": "docker run -p 3000:3000 react-19-ssr-template:prod"
  },
  "keywords": [
    "react",
    "redux",
    "ssr",
    "webpack",
    "docker",
    "tailwind",
    "express"
  ],
  "author": "altricade",
  "license": "MIT",
  "dependencies": {
    "@reduxjs/toolkit": "^2.1.0",
    "axios": "^1.6.5",
    "express": "^4.18.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.21.1",
    "redux-thunk": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@babel/preset-react": "^7.23.7",
    "@babel/preset-typescript": "^7.23.7",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/webpack": "^5.28.5",
    "@types/webpack-dev-server": "^4.7.2",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "concurrently": "^9.1.2",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "dotenv": "^16.3.1",
    "dotenv-webpack": "^8.1.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.7.6",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.33",
    "postcss-loader": "^7.3.4",
    "tailwindcss": "^3.4.1",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.0",
    "webpack-merge": "^5.10.0",
    "webpack-node-externals": "^3.0.0"
  }
}
